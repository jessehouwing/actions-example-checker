{
  "name": "jessehouwing.actions-example-checker",
  "description": "GitHub Action to validate examples in documentation",
  "version": "1.0.0",
  "author": "Jesse Houwing",
  "type": "module",
  "private": true,
  "homepage": "https://github.com/jessehouwing/actions-example-checker",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jessehouwing/actions-example-checker.git"
  },
  "bugs": {
    "url": "https://github.com/jessehouwing/actions-example-checker/issues"
  },
  "keywords": [
    "actions",
    "documentation",
    "validation"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "bundle": "npm run format:write && npm run package",
    "ci-test": "npx --node-options=--experimental-vm-modules jest",
    "coverage": "npx make-coverage-badge --output-path ./badges/coverage.svg",
    "format:write": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "lint": "npx eslint . && npx tsc --noEmit",
    "package": "npx rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "package:watch": "npm run package -- --watch",
    "test": "npx --node-options=--experimental-vm-modules jest",
    "all": "npm run format:write && npm run lint && npm run test && npm run coverage && npm run package"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^3.0.0",
    "@actions/github": "^9.0.0",
    "@actions/glob": "^0.6.1",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/js": "^10.0.1",
    "@jest/globals": "^30.2.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.4",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.15.0",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^30.2.0",
    "make-coverage-badge": "^1.2.0",
    "prettier": "^3.8.1",
    "prettier-eslint": "^16.4.2",
    "rollup": "^4.57.1",
    "ts-jest": "^29.4.6",
    "ts-jest-resolver": "^2.0.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.59.0"
  }
}
